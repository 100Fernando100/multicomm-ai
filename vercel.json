{
  "rewrites": [
    {
      "source": "/",
      "destination": "/en/index.html"
    },
    {
      "source": "/en",
      "destination": "/en/index.html"
    },
    {
      "source": "/es",
      "destination": "/es/index.html"
    },
    {
      "source": "/en/:path*",
      "destination": "/en/:path*"
    },
    {
      "source": "/es/:path*",
      "destination": "/es/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/en/",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}